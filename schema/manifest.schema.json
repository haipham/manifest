{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Vite Manifest",
    "type": "object",
    "minProperties": 1,
    "patternProperties": {
        "^.+$": {
            "type": "object",
            "properties": {
                "src": {
                    "type": "string"
                },
                "file": {
                    "type": "string"
                },
                "css": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "assets": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "isEntry": {
                    "type": "boolean"
                },
                "isDynamicEntry": {
                    "type": "boolean"
                },
                "imports": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "dynamicImports": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": [
                "file"
            ]
        }
    }
}
